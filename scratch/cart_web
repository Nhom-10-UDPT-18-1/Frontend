õ.","ANH":"https://vinmec-prod.s3.amazonaws.com/images/20190425_063710_470758_rau_xanh.max-1800x1800.jpg","created_at":"2022-07-02T18:15:30.000000Z","updated_at":"2022-07-02T18:15:30.000000Z","quantity":1,"total":21000}]